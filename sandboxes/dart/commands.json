{
    "run": {
        "engine": "docker",
        "entry": "main.dart",
        "steps": [
            {
                "box": "dart",
                "command": ["dart", "compile", "exe", "main.dart", "-o", "main"]
            },
            {
                "box": "debian",
                "version": "latest",
                "command": ["./main"]
            }
        ]
    }
}
