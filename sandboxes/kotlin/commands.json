{
    "run": {
        "engine": "docker",
        "entry": "main.kt",
        "steps": [
            {
                "box": "kotlin",
                "command": [
                    "sh",
                    "-c",
                    "kotlinc -include-runtime main.kt -d /tmp/main.jar && java -jar /tmp/main.jar"
                ],
                "timeout": 10
            }
        ]
    }
}
