{
    "run": {
        "engine": "docker",
        "entry": "main.swift",
        "steps": [
            {
                "box": "swift",
                "command": ["swiftc", "main.swift", "-o", "main"]
            },
            {
                "box": "swift-runtime",
                "version": "latest",
                "command": ["./main"]
            }
        ]
    }
}
