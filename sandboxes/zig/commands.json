{
    "run": {
        "engine": "docker",
        "entry": "main.zig",
        "steps": [
            {
                "box": "zig",
                "command": ["zig", "run", "main.zig", "-O", "ReleaseFast"]
            }
        ]
    },
    "test": {
        "engine": "docker",
        "entry": "main.zig",
        "steps": [
            {
                "box": "zig",
                "command": ["zig", "test", "main.zig"]
            }
        ]
    }
}
